{
  "name": "nuxt-ts-template",
  "version": "0.1.0",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "dev": "run-p dev:*",
    "dev:nuxt": "nuxt-ts",
    "dev:api": "aspida --watch",
    "dev:path": "pathpida --watch",
    "dev:server": "node-dev _mock_server/index.js",
    "generate:nuxt": "pathpida && aspida --build && nuxt-ts generate",
    "generate": "run-p dev:server generate:nuxt",
    "lint": "yarn lint:style",
    "lint:style": "stylelint \"**/*.{vue,scss}\" --ignore-path .gitignore",
    "start": "nuxt-ts start"
  },
  "dependencies": {
    "@aspida/axios": "^1.5.0",
    "@nuxt/typescript-runtime": "^2.0.1",
    "@nuxtjs/axios": "^5.13.1",
    "@nuxtjs/composition-api": "^0.20.1",
    "@nuxtjs/robots": "^2.4.2",
    "@nuxtjs/sitemap": "^2.4.0",
    "core-js": "^3.8.3",
    "fastify": "^3.12.0",
    "fastify-cors": "^5.2.0",
    "nuxt": "^2.15.0",
    "nuxt-mq": "^2.0.2",
    "nuxt-webfontloader": "^1.1.0",
    "vue-scrollto": "^2.20.0"
  },
  "devDependencies": {
    "@aceforth/nuxt-optimized-images": "^1.4.0",
    "@nuxt/types": "^2.15.0",
    "@nuxt/typescript-build": "^2.0.4",
    "@nuxtjs/style-resources": "^1.0.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/tailwindcss": "^3.4.2",
    "@types/sass": "^1.16.0",
    "imagemin-mozjpeg": "^9.0.0",
    "imagemin-pngquant": "^9.0.1",
    "imagemin-svgo": "^8.0.0",
    "node-dev": "^6.2.0",
    "npm-run-all": "^4.1.5",
    "nuxt-purgecss": "^1.0.0",
    "pathpida": "^0.13.0",
    "prettier": "^2.2.1",
    "responsive-loader": "^2.3.0",
    "sass": "^1.32.7",
    "sass-loader": "^10.1.1",
    "sharp": "^0.27.1",
    "stylelint": "^13.10.0",
    "stylelint-config-prettier": "^8.0.2",
    "stylelint-config-recess-order": "^2.3.0",
    "stylelint-config-recommended-scss": "^4.2.0",
    "stylelint-config-standard": "^20.0.0",
    "stylelint-prettier": "^1.1.2",
    "stylelint-scss": "^3.19.0",
    "webp-loader": "^0.6.0"
  }
}
